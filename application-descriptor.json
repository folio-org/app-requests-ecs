        "dockerPull": false,
        "dockerArgs": {"HostConfig":         {
          "Memory": 1073741824,
          "PortBindings": {"8081/tcp": [{"HostPort": "%p"}]}
        }},
        "env":         [
                    {
            "name": "JAVA_OPTIONS",
            "value": "-XX:MaxRAMPercentage=66.0"
          },
                    {
            "name": "OKAPI_URL",
            "value": "http://okapi:9130"
          },
                    {
            "name": "KAFKA_HOST",
            "value": "kafka"
          },
                    {
            "name": "KAFKA_PORT",
            "value": "9092"
          },
                    {
            "name": "KAFKA_SECURITY_PROTOCOL",
