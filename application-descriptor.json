              "modulePermissions":               [
                "requests-mediated.mediated-request.item.post",
                "requests-mediated.mediated-request.item.put",
                "requests-mediated.mediated-request.confirm.post"
              ]
            }
          ]
        },
                {
          "id": "circulation-bff-loans",
          "version": "1.6",
          "handlers":           [
                        {
              "methods": ["GET"],
              "pathPattern": "/circulation-bff/loans",
